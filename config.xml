<?xml version="1.0" encoding="utf-8" ?>
<widget xmlns:gap="http://phonegap.com/ns/1.0">

  <preference name="phonegap-version" value="3.3.0" />
  <feature name="http://api.phonegap.com/1.0/device" />

  <icon src="icon.png" />
  
    <!-- iPhone / iPod Touch -->
    <icon src="res/icons/ios/vimcomlogosingles2.png" gap:platform="ios" width="57" height="57" />
    <icon src="res/icons/ios/vimcomlogosingles2.png" gap:platform="ios" width="114" height="114" />
    <!-- iPad -->
    <icon src="res/icons/ios/vimcomlogosingles2.png" gap:platform="ios" width="72" height="72" />
    <icon src="res/icons/ios/vimcomlogosingles2.png" gap:platform="ios" width="144" height="144" />
   
    <!-- Android -->
    <icon src="res/icons/android/drawable-ldpi/vimcomlogosingles2.png" gap:platform="android" gap:density="ldpi" />
    <icon src="res/icons/android/drawable-mdpi/vimcomlogosingles2.png" gap:platform="android" gap:density="mdpi" />
    <icon src="res/icons/android/drawable-hdpi/vimcomlogosingles2.png" gap:platform="android" gap:density="hdpi" />
    <icon src="res/icons/android/drawable-xhdpi/vimcomlogosingles2.png" gap:platform="android" gap:density="xhdpi" />
  
  <gap:splash src="splash.png" />

    <gap:splash src="res/screen/ios/vimcomlogosingles2.png" gap:platform="ios" width="320" height="480" />
    <gap:splash src="res/screen/ios/vimcomlogosingles2.png" gap:platform="ios" width="680" height="960" />
    <gap:splash src="res/screen/ios/vimcomlogosingles2.png" gap:platform="ios" width="640" height="1136" />
  
    <gap:splash src="res/screen/android/drawable-ldpi/vimcomlogosingles2.png" gap:platform="android" gap:density="ldpi" />
    <gap:splash src="res/screen/android/drawable-mdpi/vimcomlogosingles2.png" gap:platform="android" gap:density="mdpi" />
    <gap:splash src="res/screen/android/drawable-hdpi/vimcomlogosingles2.png" gap:platform="android" gap:density="hdpi" />
    <gap:splash src="res/screen/android/drawable-xhdpi/vimcomlogosingles2.png" gap:platform="android" gap:density="xhdpi" />
    
</widget>